<p id="notice"><%= notice %></p>

<h1>Listing Usuario Elementos</h1>

<table>
  <thead>
    <tr>
      <th>Responsable</th>
      <th>Elemento</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuario_elementos.each do |usuario_elemento| %>
      <tr>
        <td><%= usuario_elemento.responsable %></td>
        <td><%= usuario_elemento.elemento %></td>
        <td><%= usuario_elemento.user %></td>
        <td><%= link_to 'Show', usuario_elemento %></td>
        <td><%= link_to 'Edit', edit_usuario_elemento_path(usuario_elemento) %></td>
        <td><%= link_to 'Destroy', usuario_elemento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario elemento', new_usuario_elemento_path %>
